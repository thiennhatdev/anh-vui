{
  "kind": "collectionType",
  "collectionName": "images",
  "info": {
    "singularName": "image",
    "pluralName": "images",
    "displayName": "Image",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "name_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "path": {
      "type": "string"
    },
    "size": {
      "type": "string"
    },
    "extension": {
      "type": "string"
    },
    "userId": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "images"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "imageId"
    },
    "likes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::like.like",
      "mappedBy": "imageId"
    },
    "link": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    }
  }
}
